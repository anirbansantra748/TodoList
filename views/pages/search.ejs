<% layout("/layouts/boilerplate.ejs") %>

<body>
  <div class="dashboard-show row">
    <div class="all-tasks overflow-y-scroll">
      <h2>Search result for <%= searchTerm %></h2>
      <br>
      <ul>
        <% tasks.forEach(task =>{ %>
        <li><%= task.task %> -> <%= task.startTime %> &nbsp;- <%= task.endTime %></li>
        <br>
        <% }); %>
      </ul>
    </div>
  </div>
</body>
